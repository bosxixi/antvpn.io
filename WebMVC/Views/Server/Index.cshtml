
<div class="container">
    <h1>Servers</h1>

    <div class="row">
        @if(ViewData["logins"] != null)
        {
        foreach(var server in ViewData["logins"] as IEnumerable<WebMVC.ViewModels.Server>)
        {
    <div class="col-md-3">
        <figure class="media">
            <div class="media-img media-img-has-label ratio-movie">
                <a href="#">
                    <img src="~/images/icon-virtual-machines.png" alt="" />
                </a>
                <span class="media-img-label">@server.Name</span>
                <div class="progress progress-media">
                    <div class="progress-bar" role="progressbar"
                         aria-valuenow="60" aria-valuemin="0" aria-valuemax="100"
                         style="width: 60%;"></div>
                    <!-- Accessibility -->
                    <span class="sr-only">60%</span>
                </div>
            </div>
            <figcaption>
                <h4 class="media-header">
                    @*<a href="#">Header</a>*@
                    @server.Domain
                </h4>
                <div class="media-price">
                    @server.Description
                </div>
            </figcaption>
        </figure>
    </div>
        }

        }

    </div>

</div>